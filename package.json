{
  "name": "homepage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env -c -- bash -c 'next dev -H $NEXTJS_HOST -p $NEXTJS_PORT'",
    "start": "dotenv -e .env -c -- bash -c 'next start -H $NEXTJS_HOST -p $NEXTJS_PORT'",
    "build": "dotenv -e .env -c -- bash -c 'next build'",
    "lint": "next lint",
    "_upload": "dotenv -e .env -c -- bash -c 'rclone sync out $RCLONE_DESTINATION --progress'"
  },
  "dependencies": {
    "@types/markdown-it": "^12.2.3",
    "@types/node": "20.3.3",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "dotenv-cli": "^7.2.1",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.7",
    "markdown-it": "^13.0.1",
    "next": "^13.4.7",
    "react": "18.2.0",
    "react-devicons": "^2.14.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.10.1",
    "sharp": "^0.32.1",
    "typescript": "5.1.6",
    "zustand": "^4.3.8"
  }
}
